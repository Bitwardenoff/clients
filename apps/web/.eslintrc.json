{
  "env": {
    "browser": true
  },
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": [
          "**/app/core/*",
          "**/app/shared/*",
          "app/tools/reports",
          "**/organizations/settings/*",
          "**/organizations/policies/*",
          "@bitwarden/web-vault/*",
          "src/**/*",
          "bitwarden_license"
        ]
      }
    ]
  }
}
